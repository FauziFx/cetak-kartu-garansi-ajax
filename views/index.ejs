<!DOCTYPE html>
<html>
  <head>
    <script src="../javascripts/color-modes.js"></script>
    <title><%= title %></title>
    <link rel="stylesheet" href="../stylesheets/style.css" />
    <link href="../bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="../fontawesome/js/all.min.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="../stylesheets/sweetalert2.min.css" />
    <meta name="theme-color" content="#712cf9" />
  </head>
  <body>
    <%- include('theme-switch') %>
    <div class="container py-3">
      <%- include('header') %> <%- include('modal-edit') %>

      <div class="form-group mb-2">
        <input
          type="text"
          name="search_box"
          id="search_box"
          class="form-control"
          placeholder="Type your search query here"
          autofocus
        />
      </div>
      <div class="table-responsive">
        <table
          class="table table-striped table-hover table-sm"
          id="sample_data"
        >
          <thead>
            <tr>
              <th>Tanggal</th>
              <th>Optik</th>
              <th>Nama</th>
              <th>Frame</th>
              <th>Lensa</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody></tbody>
        </table>
        <div class="d-flex justify-content-between" id="pagination"></div>
      </div>
    </div>
    <%- include('modal-tambah') %> <%- include('modal-preview') %>
    <script src="../javascripts/jquery.min.js"></script>
    <script src="../bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="../javascripts/loadingoverlay.min.js"></script>
    <script src="../javascripts/sweetalert2.all.min.js"></script>
    <script src="../javascripts/moment.min.js"></script>
    <script src="../javascripts/moment-timezone-with-data.js"></script>
    <script src="../javascripts/script.js"></script>
  </body>
</html>
